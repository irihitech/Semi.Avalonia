    <SolidColorBrush x:Key="ComboBoxPopupBackground" Color="White" />
    <SolidColorBrush x:Key="ComboBoxPopupBorderBrush" Opacity="0.08" Color="#1C1F23" />

    <SolidColorBrush x:Key="ComboBoxItemForeground" Color="#1C1F23" />

